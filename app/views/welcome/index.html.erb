<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<p><%= link_to 'Sign out', destroy_user_session_path, method: :delete %></p>

<table>
  <tr>
    <th>username</th>
    <th>password</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.encrypted_password %></td>
    </tr>
  <% end %>
</table>
    
